{
  "name": "actions-cache-s4",
  "packageManager": "npm@8.11.0",
  "version": "0.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "dist/save.js",
  "scripts": {
    "proto:gen": "make gen",
    "build": "tsc",
    "package": "npm-run-all -p package:save package:restore",
    "package:save": "ncc build --source-map --license licenses.txt -o dist/save src/save.ts",
    "package:restore": "ncc build --source-map --license licenses.txt -o dist/restore src/restore.ts",
    "build:all": "npm-run-all -s proto:gen build package",
    "dev:save": "ts-node-dev --respawn -r tsconfig-paths/register src/save.ts",
    "dev:restore": "ts-node-dev --respawn -r tsconfig-paths/register src/restore.ts",
    "start": "npm dev",
    "lint": "eslint --cache src/**/*.ts",
    "lint:fix": "eslint --cache --fix src/**/*.ts",
    "prettier": "prettier --write --ignore-path .prettierignore './**/*.ts'",
    "prettier:check": "prettier --check --ignore-path .prettierignore './**/*.ts'",
    "format": "npm run lint:fix && npm run prettier",
    "jest": "jest",
    "jest:watch": "jest --watchAll",
    "test": "npm-run-all -p lint prettier:check jest",
    "all": "npm-run-all -s proto:gen build format package test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CyberAgent-Infosys/actions-cache-s4.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "CA Advance",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/glob": "^0.4.0",
    "@actions/io": "^1.1.2",
    "@grpc/grpc-js": "^1.8.11",
    "google-protobuf": "^3.21.2",
    "grpc-tools": "^1.12.4",
    "node-fetch": "^2.6.9",
    "ts-protoc-gen": "^0.15.0"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.6",
    "@types/node": "^18.14.6",
    "@types/node-fetch": "^2.6.2",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/parser": "^5.54.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.35.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-github": "^4.6.1",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.4.3",
    "js-yaml": "^4.1.0",
    "nock": "^13.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5",
    "typescript-transform-paths": "^3.4.6"
  }
}
